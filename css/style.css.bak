@font-face {
  font-family: 'RussoOne-Regular';
  src: url('../fonts/RussoOne-Regular.eot');
  src: url('../fonts/RussoOne-Regular.eot?#iefix') format('embedded-opentype'),
       url('../fonts/RussoOne-Regular.svg#RussoOne-Regular') format('svg'),
       url('../fonts/RussoOne-Regular.ttf') format('truetype'),
       url('../fonts/RussoOne-Regular.woff') format('woff'),
       url('../fonts/RussoOne-Regular.woff2') format('woff2');
  font-weight: normal;
  font-style: normal;
}



body {
	background:#eee; 
	font-size: 14px;  
    font-family: "Open Sans",sans-serif;
}

.border-1 {
	border: 1px solid #212529; 
	padding: 10px;
	margin-bottom:10px; 
}
.border-2 {
	border: 1px solid #212529; 
	padding: 10px; 
	margin-bottom:10px; 
}
.top_backround {
	overflow: hidden;
	background:#eee; 
	height:30px;
	position: fixed;
	z-index:999; 	
	top: 0;
	width: 100%;
}
.board_marker {
	background:#f6f6f6; 
	color:#888888; 
	height:15px; 
	box-shadow:2px 0px 2px 0px ; 
}

.modal-footer {
    padding: 2px;
}
/*Table*/
.table th, .table td {
    padding: 0.3rem;
    vertical-align: top;
    border-top: 1px solid #dee2e6;
}
/*Login*/
.Wrapper{
	width:400px; 
	margin:0px auto; 
	margin-top:100px; 
	box-shadow: 0px 0px 5px 2px; 
}
.card-wrapper {
	background: #fff;
	padding: 20px;
	border-radius: 5px;
	padding-bottom: 3px;
	padding-left: 15px;
	padding-top: 0;
}
.card-wrapper h4 p {
	padding:0px; 
	margin:0px; 
}
.card-wrapper h4  {
	color: #ff8800; 
}
.middle_area {
	padding: 20px; 
}
.top_area {
	background:#31353D; 
	padding:20px; 
	font-size: 20px; 
	color:#fff; 
}
/*End Login*/

.page_wrapper{
	box-shadow: 0px 0px 5px 1px; 
	background:#fff; 
	padding: 20px; 
	height:200%;
}
.height{
	height:100%; 
}

.nav-tabs {
    border-bottom: 1px solid #ffc107;
}



/*Data Table*/


.page-item.active .page-link {
    z-index: 3;
    color: #fff;
    background-color:#059605;
    border-color: #059605;
}

.page-link {
    color: #fff;
    background-color: #ffc107;
    border: 1px solid #dee2e6;
}
.dataTables_wrapper .dataTables_paginate 
.paginate_button {
    padding: 0em 0em;
    text-decoration: none !important;
    cursor: pointer;
    *cursor: hand;
    color: #333 
!important;
    border: 1px solid transparent;
    border-radius: 2px;
}


table.dataTable>thead>tr>th:not(.sorting_disabled), table.dataTable>thead>tr>td:not(.sorting_disabled) {
 
    padding: 1px;
}